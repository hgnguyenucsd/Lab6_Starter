            },
            {
              "@type": "HowToStep",
              "text": "Simmer remaining soup UNCOVERED for 40 minutes.",
              "name": "Simmer remaining soup UNCOVERED for 40 minutes.",
              "url": "https://www.recipetineats.com/vietnamese-pho-recipe/#wprm-recipe-36115-step-2-5"
            },
            {
              "@type": "HowToStep",
              "text": "Strain broth into another pot, discard bones and spices. Should be about 2.5 litres / 2.65 quarts (10 cups), if loads more, reduce.",
              "name": "Strain broth into another pot, discard bones and spices. Should be about 2.5 litres / 2.65 quarts (10 cups), if loads more, reduce.",

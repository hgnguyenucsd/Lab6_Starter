            }
          ]
        },
        {
          "@type": "HowToSection",
          "name": "Broth:",
          "itemListElement": [
            {
              "@type": "HowToStep",
              "text": "Wipe pot clean, bring 3.5 litres / 3.75 quarts water to boil.",
              "name": "Wipe pot clean, bring 3.5 litres / 3.75 quarts water to boil.",
              "url": "https://www.recipetineats.com/vietnamese-pho-recipe/#wprm-recipe-36115-step-2-0"
            },
            {
              "@type": "HowToStep",
              "text": "Add bones and brisket, onion, ginger, Spices",
              "name": "Add bones and brisket, onion, ginger, Spices",
              "url": "https://www.recipetineats.com/vietnamese-pho-recipe/#wprm-recipe-36115-step-2-1"
            },
            {
              "@type": "HowToStep",
              "text": "Add onion, ginger, Spices, sugar and salt - water should just barely cover everything.",
              "name": "Add onion, ginger, Spices, sugar and salt - water should just barely cover everything.",
              "url": "https://www.recipetineats.com/vietnamese-pho-recipe/#wprm-recipe-36115-step-2-2"
            },
            {
              "@type": "HowToStep",
              "text": "Cover with lid, simmer 3 hours.",
              "name": "Cover with lid, simmer 3 hours.",

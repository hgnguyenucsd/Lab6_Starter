          "name": "Aromatics",
          "itemListElement": [
            {
              "@type": "HowToStep",
              "text": "Heat a heavy based skillet over high heat (no oil) until smoking.",
              "name": "Heat a heavy based skillet over high heat (no oil) until smoking.",

            {
              "@type": "HowToStep",
              "text": "Place onion and ginger in pan cut side down. Cook for a few minutes until it's charred, then turn. Remove and set aside.",
              "name": "Place onion and ginger in pan cut side down. Cook for a few minutes until it's charred, then turn. Remove and set aside.",
              "url": "https://www.recipetineats.com/vietnamese-pho-recipe/#wprm-recipe-36115-step-0-1"
            },
            {
              "@type": "HowToStep",
              "text": "Toast Spices lightly in a dry skillet over medium high heat for 3 minutes.&nbsp;",
              "name": "Toast Spices lightly in a dry skillet over medium high heat for 3 minutes.&nbsp;",
              "url": "https://www.recipetineats.com/vietnamese-pho-recipe/#wprm-recipe-36115-step-0-2"
